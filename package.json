{
  "name": "yummy",
  "description": "Yummy recipes",
  "license": "MIT",
  "author": "Dyrksy",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "lint": "eslint *.js 'src/**/*.js' 'plugins/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "gatsby build",
    "test:build": "if [ -z \"$CI_SKIP_BUILD\" ]; then GATSBY_SOURCE=test gatsby build; fi",
    "test:serve": "gatsby serve --port 8001",
    "test:develop": "gatsby develop --port 8001",
    "deploy": "bash -x ./scripts/deploy.sh",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "pup:run": "node puppeteer/index.js",
    "test:visual": "yarn test:build && start-server-and-test 'yarn test:serve' http://localhost:8001 'yarn pup:run'",
    "test:visual:update": "UPDATE_SNAPSHOTS=true yarn test:visual",
    "test:e2e": "GATSBY_SOURCE=test start-server-and-test 'yarn test:develop' http://localhost:8001 'yarn cy:open'",
    "test:e2e:ci": "yarn test:build && start-server-and-test 'yarn test:serve' http://localhost:8001 'yarn cy:run'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@svgr/webpack": "^4.3.0",
    "emotion": "9",
    "emotion-server": "9",
    "fuse.js": "^3.3.0",
    "gatsby": "^2.3.29",
    "gatsby-image": "^2.0.25",
    "gatsby-plugin-feed": "^2.0.11",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.16",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typography": "^2.2.4",
    "gatsby-source-filesystem": "^2.0.21",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "jsdom": "^13.2.0",
    "node-sass": "^4.11.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-select": "2.3.0",
    "react-testing-library": "^6.0.0",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18",
    "underscore.string": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-gatsby": "^0.1.7",
    "blink-diff": "^1.0.13",
    "css-loader": "^2.1.1",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.3.6",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "gatsby-plugin-eslint": "^2.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "puppeteer": "^1.13.0",
    "react-test-renderer": "^16.8.2",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.7"
  }
}
